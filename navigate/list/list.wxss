page{
  box-sizing: border-box;
  font-family: PingFangSC-Regular;
  background-color: #eeeeee;
}
.t-vip-bg{
  background: url("http://static.bddvip.com/image/20181224/20181224121940_966.png") no-repeat;
  background-size: 100% 100%;
}
.t-discount-bg{
  background: url("http://static.bddvip.com/image/20181224/20181224144842_698.png") no-repeat;
  background-size: 100% 100%;
}
.t-invalid-bg{
  background: url("http://static.bddvip.com/image/20181224/20181224155146_953.png") no-repeat;
  background-size: 100% 100%;
}
.c-item{
  width: 100%;
  height: 220rpx;
  margin-top: 20rpx;
  display: flex;
  flex-direction: row;
  overflow: hidden;
  position: relative;
}
.left{
  width: 200rpx;
  height: 100%;
  color: #FFFFFF;
  letter-spacing: 0;
  text-align: center;
}
.category{
  line-height: 156rpx;
  font-size: 72rpx;
  white-space: nowrap;
  overflow: hidden;
}
.c-type{
  font-size: 26rpx;
  line-height: 64rpx;
}
.right{
  width: 405rpx;
  height: 100%;
  margin-left: 40rpx;
  display: flex;
  flex-direction: column;
}
.title{
font-family: Helvetica;
font-size: 42rpx;
color: #000000;
margin-top: 28rpx;
white-space: nowrap;
overflow: hidden;
}
.desc{
  margin-top: 12rpx;
  font-size: 26rpx;
  color: #808080;
}
.t-invalid-bg .title,.t-invalid-bg .desc{
opacity: 0.5;
}
.tag{
  position: absolute;
  top: 57rpx;
  right: 37rpx;
}
.tag image{
  width: 160rpx;
  height: 87rpx;
}
.myCoupon{
  height: 73rpx;
  display: flex;
  background: #FFFFFF;
  font-size:24rpx;
  line-height: 73rpx;
  justify-content: space-between;
  padding: 0 150rpx;
}
.conetext{
  padding: 10rpx 30rpx;
  box-sizing: border-box;
  font-family: PingFangSC-Regular;
  background-color: #eeeeee;
}
.selected{
  color:#BF9F56;
}
.no-coupons{
  font-size: 32rpx;
  text-align: center;
  margin-top: 130rpx;
}
.endTime{
  font-size:26rpx;
  font-weight:400;
  color:#808080;
  line-height: 64rpx;
}
.coupon-detail{
  height: 156rpx;
}